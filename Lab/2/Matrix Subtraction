#include<stdio.h>

main()
{
    int r, c, i, j, first[100][100], second[100][100];
    scanf("%d %d", &r, &c);
    for(i=0; i<r; i++){
        for(j=0; j<c; j++)scanf("%d", &first[i][j]);
    }
    for(i=0; i<r; i++){
        for(j=0; j<c; j++)scanf("%d", &second[i][j]);
    }
    for(i=0; i<r; i++){
        for(j=0; j<c; j++)printf("%d ", first[i][j]-second[i][j]);
        printf( "\n");
    }
}
