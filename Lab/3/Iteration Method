#include<stdio.h>
#define func(x) (cos(x)/exp(x))
void Iteraion_Method(double x)
{
    double d=2.0,ck[1000];
    int i=1;

    ck[0]=func(x);
    while(d>0.01)
    {
        ck[i]=func(ck[i-1]);
        d=fabs(ck[i]-ck[i-1]);
        i++;
    }
    printf("%lf\n", ck[i-1]);
}
main()
{
    Iteraion_Method(0.5174);

}

