#include<stdio.h>

double Call_vl(double vl, int i, double x[])
{
    double mul=1.0;
    for(int j=1; j<=i; j++)mul*=(vl-x[j-1]);
    return mul;
}
 main()
{
    int n, i,j; double x[100], y[100][100], u, sum,vl;
    scanf("%d", &n);
    for(i=0; i<n; i++)scanf("%lf %lf", &x[i], &y[i][0]);
    scanf("%lf", &vl);

    for(i=1; i<n; i++){
        for(j=0; j<(n-i); j++)y[j][i]=(y[j+1][i-1]-y[j][i-1])/(x[j+i]-x[j]);
    }

    sum=y[0][0];

    for(i=1; i<n; i++)
    {
        sum+=Call_vl(vl, i, x)*y[0][i];
    }
    printf("%lf\n", sum);

}
