#include<stdio.h>
#define func(x) (1/(1+x*x))
int main()
{
    double a=0, b=1,h, sum=0, n=6,vl;
    h=(b-a)/n;
    for(int i=0; i<=n; i++)
    {
        vl= func(a+i*h);
        if(i==0 || i==n)sum+=vl;
        else if (i%2==1)sum+=4*vl;
        else sum+=2*vl;
    }
    sum*=(h/3);
    printf("%lf", sum);
}
