#include<bits/stdc++.h>
using namespace std;

double Bisecetion(int a, int b)
{
    double up=(double)b,low=(double)a,mid=(up+low)/2,vl;
    for(int i=0; i<100; i++)
    {
        mid=(up+low)/2;
        vl=mid*mid-4*mid-10;
        if(vl>0)low=mid;
        else up=mid;
        if(abs(vl-0.0001)<=0.001){
            return mid;
        }

    }

}
int main()
{
     int a=-2,b=-1;
        double vl=Bisecetion(a,b);
        printf("%f", vl);

    return 0;
}
